/* pages/cart/cart.wxss */

.receive {
  font-size: 28rpx;
  font-weight: 400;
  width: 100%;
  position: relative;
  background-image: url("data:image/png;base64,"),
                      url("data:image/png;base64,");
  background-position: left top, left bottom;
  background-repeat: repeat-x;
  -webkit-background-size: auto 6rpx;
  background-size: auto 6rpx;
  background-color: #fff;
  margin-bottom: 20rpx;
}

.receive .tr {
  display: flex;
}

.receive .th {
  padding: 14rpx 20rpx 14rpx 30rpx;
  text-align: left;
  width: 28%;
  box-sizing: border-box;
}

.receive .more {
  position: absolute;
  right: 20rpx;
  top: 50%;
  transform: translate(0, -50%);
}

.receive .td {
  flex: 1;
  padding: 14rpx 0;
}

.cart-group {
  padding-top: 10rpx;
  font-size: 24rpx;
  background-color: #fff;
}

.cart-group .group-name {
  padding-left: 28rpx;
  overflow: hidden;
  padding-right: 24rpx;
}

.cart-group .group-name>text {
  float: left;
  color: #999;
}

.cart-group .group-name>text:before {
  content: '';
  display: inline-block;
  width: 8rpx;
  height: 24rpx;
  border-radius: 4rpx;
  margin-right: 10rpx;
  background-color: #ffd600;
  vertical-align: -4rpx;
}

.cart-group .group-name >var {
  float: right;
  height: 48rpx;
  line-height: 48rpx;
  border: 1px solid #ff3800;
  border-radius: 24rpx;
  width: 140rpx;
  text-align: center;
}

.cart-group .group-proptext {
  color: #999;
  padding-left: 28rpx;
  height: 40rpx;
  line-height: 40rpx;
}

.group-receive {
  padding-left: 28rpx;
  font-size: 28rpx;
  height: 100rpx;
  line-height: 100rpx;
  border-top: 1px solid #e0e0e0;
  position: relative;
}

.group-receive .time {
  font-size: 28rpx;
}

.group-receive .group-receive-select {
  border: 1px solid #ff0;
  font-size: 28rpx;
  background-color: #fff;
  position: absolute;
  top: 100rpx;
  left: 0;
  right: 0;
  height: 500rpx;
  overflow-x: hidden;
  overflow-y: scroll;
  z-index: 5;
}

.group-receive .group-receive-select view {
  height: 50rpx;
  line-height: 50rpx;
  padding-left: 30rpx;
}

.group-receive .group-receive-select view:first-child,
.group-receive .group-receive-select view:nth-last-child(5),
.group-receive .group-receive-select view:nth-child(7) {
  padding-left: 0;
  font-weight: bold;
}

.group-receive .group-receive-select view.active {
  background: rgba(0, 255, 255, 0.4);
  color: #fff;
}

.group-receive .group-receive-ext {
  float: right;
  margin-right: 30rpx;
}

.group-comment {
  height: 100rpx;
  line-height: 100rpx;
  padding-left: 28rpx;
  font-size: 28rpx;
  display: -webkit-flex;
  display: flex;
  border-top: 1px solid #e0e0e0;
  border-bottom: 1px solid #e0e0e0;
  align-items: center;
}

.group-comment .group-comment-txt {
  float: left;
}

.group-comment .group-comment-input-wrap {
  flex: 1;
  padding-right: 44rpx;
}

.group-comment .group-comment-input {
  height: 64rpx;
  border: 1px solid #cecece;
  border-radius: 10rpx;
  width: 100%;
  box-sizing: border-box;
  outline: none;
  text-indent: 10rpx;
  font-size: 28rpx;
}

/* 商品列表 */

.pro_item {
  display: flex;
  align-items: center;
  height: 160rpx;
  border-bottom: 1px solid #e0e0e0;
}

.pro_item > view:first-child {
  width: 20%;
	height: 100%;
  background-image: url("data:image/png;base64,");
  background-position: center center;
  background-repeat: no-repeat;
  background-size: 55rpx 55rpx;
}

.pro_item > view.active {
  background-image: url("data:image/png;base64,");
}

.pro_item .group-item-img {
  width: 20%;
}

.pro_item .group-item-img >image {
  width: 100rpx;
  height: 100rpx;
}

.group-item-detail {
  width: 60%;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  padding-right: 24rpx;
}

.group-item-detail>view {
  height: 80rpx;
  line-height: 80rpx;
  padding-left: 30rpx;
}

.group-item-detail .name {
  font-size: 26rpx;
}

.group-item-detail .product-specifics-wrap {
  overflow: hidden;
}

.group-item-detail  .product-specifics {
  float: left;
  width: 30%;
  overflow: hidden;
}

.group-item-detail  .product-specifics >text {
  display: block;
  color: #858585;
  margin-bottom: 20rpx;
}

.group-item-detail  .product-specifics >var {
  color: #f40;
}

.product-operates {
  text-align: right;
}

.product-operates  .inner {
  display: inline-block;
  font-size: 48rpx;
  width: 58rpx;
  height: 58rpx;
  line-height: 54rpx;
  color: #ff4300;
  text-align: center;
  /*padding: 0.8rem;*/
  border: 1px solid #ffbe89;
  border-radius: 50%;
}

.product-operates-item {
  display: inline-block;
  height: 56rpx;
  line-height: 56rpx;
  vertical-align: top;
  font-size: 24rpx;
  margin: 0 10rpx;
}

.group-footer {
  height: 100rpx;
  line-height: 100rpx;
  font-size: 32rpx;
  padding-left: 28rpx;
  margin-bottom: 20rpx;
}

.group-footer > text:first-child {
  display: inline-block;
  width: 100rpx;
  padding-left: 100rpx;
  background-size: 60rpx 60rpx;
  background-repeat: no-repeat;
  background-position: left center;
}

.group-footer .selectAll-false {
  background-image: url("data:image/png;base64,");
}

.group-footer .selectAll-true {
  background-image: url("data:image/png;base64,");
}

.group-footer  .group-btn {
  height: 100%;
  float: right;
  width: 200rpx;
  text-align: center;
  background-color: #ff0;
}

.group-footer .group-btn1 {
	background-color: #aaa;
}
