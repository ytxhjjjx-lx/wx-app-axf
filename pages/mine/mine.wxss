/* pages/mine/mine.wxss */

.mine {
  font-size: 28rpx;
}

.userinfo {
  height: 200rpx;
  position: relative;
  background-color: #ffd600;
}

.userinfo  .user {
  height: 120rpx;
  padding-left: 180rpx;
  padding-top: 40rpx;
  font-size: 30rpx;
  font-weight: 700;
  color: #fff;
  background: url("data:image/png;base64,") 30rpx bottom no-repeat;
  -webkit-background-size: auto 120rpx;
  background-size: auto 120rpx;
}

.userinfo .user:after {
  background: url("data:image/png;base64,") 0 0 no-repeat;
  -webkit-background-size: 100% 100%;
  background-size: 100% 100%;
  content: '';
  position: absolute;
  width: 22rpx;
  height: 38rpx;
  top: 50%;
  right: 40rpx;
  transform: translateY(-50%);
}

.userinfo .user .user-level {
  color: #f40;
  background: url("data:image/png;base64,") left center no-repeat;
  -webkit-background-size: auto 100%;
  background-size: auto 100%;
  line-height: 56rpx;
  padding-left: 58rpx;
  font-size: 20rpx;
  font-weight: normal;
}

.userinfo .user .user-phone {
  margin: 14rpx 0;
  line-height: 1;
}

.favorite {
  display: flex;
  height: 70rpx;
  align-items: center;
  color: #fff;
  font-size: 26rpx;
  background-color: #f3c800;
}

.favorite .item {
  width: 50%;
  display: flex;
  justify-content: center;
}

.favorite .goodFavorite .inner {
  background: url("data:image/png;base64,") left center no-repeat;
  -webkit-background-size: auto 36rpx;
  background-size: auto 36rpx;
  padding-left: 60rpx;
}

.favorite .shopFavorite .inner {
  background: url("data:image/png;base64,") left center no-repeat;
  -webkit-background-size: auto 36rpx;
  background-size: auto 36rpx;
  padding-left: 60rpx;
}

.favorite .split {
  width: 1px;
  height: 60%;
  -ms-align-self: center;
  align-self: center;
  background-color: #fff;
}

.mine-content {
  position: absolute;
  padding-top: 20rpx;
  top: 270rpx;
  left: 0;
  right: 0;
  bottom: 98rpx;
  background-color: #efefef;
  overflow-y: auto;
  overflow-x: hidden;
}

.mine-order {
  background-color: #fff;
  font-size: 28rpx;
}

.mine-order  .more {
  background: url("data:image/png;base64,") right center no-repeat;
  background-size: auto 20rpx;
  border-bottom: 1px solid #e0e0e0;
  padding: 28rpx 50rpx 28rpx 30rpx;
}

.mine-order .more .more_order {
  float: right;
	line-height: 16rpx;
  color: #aaa;
}

.mine-order .order-types {
  overflow: hidden;
	display: flex;
}

.mine-order .order-types .order-type {
  position: relative;
  flex: 1;
  padding-top: 13%;
  text-align: center;
  line-height: 60rpx;
  font-size: 24rpx;
	background-repeat: no-repeat;
	background-position: center 40%;
  background-size: auto 25%;
}

.order-type-11 {
  background-image: url("data:image/png;base64,");
}

.order-type-12 {
  background-image: url("data:image/png;base64,");
}

.order-type-13 {
  background-image: url("data:image/png;base64,");
}

.order-type-14 {
  background-image: url("data:image/png;base64,");
}

.block {
  background-color: #fff;
  margin-top: 20rpx;
  display: flex;
  flex-flow: row wrap;
}

.block > view, .block > navigator {
  width: 25%;
  height: 200rpx;
  display: flex;
  flex-flow: column wrap;
  justify-content: center;
  align-items: center;
}

.block view {
  font-size: 26rpx;
}

.block view image {
	width: 60rpx;
  height: 50rpx;
}

.block view .mine-item {
  margin-bottom: 10rpx;
}

.unlogin {
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 28rpx;
  text-align: center;
  margin: 40rpx 0 20rpx 0;
  background-color: #fff;
}
