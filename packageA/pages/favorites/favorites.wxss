/* packageA/pages/favorites/favorites.wxss */

#main_content.btm {
  bottom: 100rpx;
}

#main_content .products .product-item-wrap {
  align-items: center;
  height: 160rpx;
  border-bottom: 1px solid #e0e0e0;
}

#main_content .product-item-wrap>view:first-child {
  width: 15%;
  height: 100%;
  background-image: url("data:image/png;base64,");
  background-repeat: no-repeat;
  background-size: 50rpx 50rpx;
  background-position: center center;
}

#main_content  .product-item-wrap>view.selected {
  background-image: url("data:image/png;base64,");
}

#main_content  .product-item-wrap .product-image {
  width: 25%;
	text-align: center;
}

#main_content  .product-item-wrap  .product-image image {
  width: 120rpx;
  height: 120rpx;
}

#main_content  .product-item-wrap .pro-detail {
  width: 60%;
  flex-wrap: wrap;
  justify-content: space-between;
  padding-right: 24rpx;
  font-size: 24rpx;
}

#main_content .product-item-wrap .pro-detail .theme-font-gray {
  color: #999;
}

#main_content .product-item-wrap .pro-detail .theme-font {
  color: #f40;
}

.footer {
  position: fixed;
  height: 100rpx;
  left: 0;
  right: 0;
  bottom: 0;
  padding-left: 40rpx;
}

.footer text:first-child {
  display: inline-block;
  width: 100rpx;
  padding-left: 60rpx;
  font-size: 28rpx;
  line-height: 100rpx;
  background-image: url("data:image/png;base64,");
  -webkit-background-size: 50rpx 50rpx;
  background-size: 50rpx 50rpx;
  background-repeat: no-repeat;
  background-position: left center;
}

.footer text.selectAll-true {
  background-image: url('data:image/png;base64,');
}

.footer .themebtn {
  border-radius: 10rpx;
  color: #000;
  text-align: center;
  padding: 20rpx 48rpx;
  font-size: 28rpx;
  position: absolute;
  right: 20rpx;
  top: 14rpx;
}
