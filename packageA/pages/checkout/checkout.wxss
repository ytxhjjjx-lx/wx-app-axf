/* packageA/pages/checkout/checkout.wxss */
.main_content {
	font-size: 30rpx;
	bottom: 120rpx;
	color: #333;
}

.wrap {
	padding-left: 30rpx;
}

.wrap-head {
	font-size: 26rpx;
	font-weight: bold;
	height: 60rpx;
	line-height: 60rpx;
}

.details {
	margin-bottom: 20rpx;
}

.pro-details .cont .name {
	flex: 1;
}

.pro-details .cont .num {
	width: 10%;
	text-align: left;
}

.pro-details .cont .price {
	width: 20%;
	text-align: left;
	margin-left: 20rpx;
}

.cont {
	height: 80rpx;
	line-height: 80rpx;
}

.fee {
	padding-right: 30rpx;
}

.fee text:last-child {
	float: right;
}

.main_content .pay-way .payWay {
	background-position: left center;
	background-size: 40rpx 40rpx;
	background-repeat: no-repeat;
	padding-left: 50rpx;
	position: relative;
}

.main_content .pay-way .payWay image{
	position: absolute;
	right: 30rpx;
	top: 50%;
	transform: translateY(-50%);
	width: 40rpx;
	height: 40rpx;
}

.pay-way1 {
	background-image: url('data:image/png;base64,');
}

.pay-way2 {
	background-image: url('data:image/png;base64,') ;
}

.pay-way3 {
	background-image: url('data:image/png;base64,');
}

.coupons {
	margin: 20rpx 0;
	padding-left: 30rpx;
}

.coupons view {
	padding-right: 30rpx;
}

.coupons view .no-coupons {
	float: right;
}

.place-order {
	position: fixed;
	height: 120rpx;
	line-height: 120rpx;
	bottom: 0;
	left: 0;
	right: 0;
}

.place-order view:first-child {
	padding-left: 30rpx;
	flex: 2;
}

.place-order view:last-child {
	text-align: center;
	flex: 1;	
}