/* components/header-gray/header-gray.wxss */
.pub-header{
  height: 90rpx;
  line-height: 90rpx;
  background-color: #f9fafd;
  position: fixed;
  width: 100%;
  text-align: center;
  z-index: 2;
  font-size: 30rpx;
  font-weight: 700;
}
.header-content{
  position: absolute;
  left: 120rpx;
  right: 120rpx;
}
.header-icon{
  position: absolute;
  top: 0;
  width: 120rpx;
  height: 88rpx;
  background-repeat: no-repeat;
  -webkit-background-size: auto 50rpx;
  background-size: auto 50rpx;
  background-position: left center;
}
.header-left{
  background-image: url("data:image/png;base64,");
  left: 12rpx;
}